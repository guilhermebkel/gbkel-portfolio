{
	"redirects": [
		{
			"source": "/thumbnail/(.*).png",
			"destination": "/api/thumbnail?slug=$1"
		}
	],
	"rewrites": [
		{
			"source": "/sitemap.xml",
			"destination": "/api/sitemap"
		},
		{
			"source": "/rss.xml",
			"destination": "/api/rss"
		}
	],
	"functions": {
		"pages/api/*.ts": {
			"includeFiles": "lib/**"
		}
	}
}
