{
	"redirects": [
		{
			"source": "/thumbnail/(.*.png)",
			"destination": "/api/thumbnail?file=$1"
		},
		{
			"source": "/service-worker/(.*.js)",
			"destination": "/api/service-worker?file=$1"
		}
	],
	"rewrites": [
		{
			"source": "/sitemap.xml",
			"destination": "/api/sitemap"
		},
		{
			"source": "/rss.xml",
			"destination": "/api/rss"
		},
		{
			"source": "/manifest.json",
			"destination": "/api/manifest"
		}
	]
}
